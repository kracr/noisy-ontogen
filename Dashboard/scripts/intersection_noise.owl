  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="pizza.owl#hasTopping"/>
      <owl:someValuesFrom rdf:resource="pizza.owl#MozzarellaTopping"/>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="pizza.owl#hasTopping"/>
      <owl:someValuesFrom rdf:resource="pizza.owl#PeperoniSausageTopping"/>
    </owl:Restriction>
