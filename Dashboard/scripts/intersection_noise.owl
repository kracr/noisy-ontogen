    <owl:Restriction>
      <owl:onProperty rdf:resource="pizza.owl#hasTopping"/>
      <owl:someValuesFrom rdf:resource="pizza.owl#TomatoTopping"/>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="pizza.owl#hasTopping"/>
      <owl:allValuesFrom>
        <owl:Class>
          <owl:unionOf rdf:parseType="Collection">
            <rdf:Description rdf:about="pizza.owl#MozzarellaTopping"/>
            <rdf:Description rdf:about="pizza.owl#PeperoniSausageTopping"/>
            <rdf:Description rdf:about="pizza.owl#TomatoTopping"/>
          </owl:unionOf>
        </owl:Class>
      </owl:allValuesFrom>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="pizza.owl#hasCountryOfOrigin"/>
      <owl:hasValue rdf:resource="pizza.owl#America"/>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:label xml:lang="en">American</rdfs:label>
  <rdfs:label xml:lang="pt">Americana</rdfs:label>
  <core:altLabel xml:lang="en">American</core:altLabel>
  <core:altLabel xml:lang="en">American Pizza</core:altLabel>
  <core:prefLabel xml:lang="en">American</core:prefLabel>
</owl:Class>

